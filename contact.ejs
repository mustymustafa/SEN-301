          <input type="text" class="form-control" name = "title" placeholder="Title" required>

          </input>

        </div>





        <div class="form-group">

          <label>Your Message</label>

          <br>

          <textarea class="form-control" name = "message" placeholder="Type away..." clear>

          </textarea>

        </div>

        <button class="outsourceng-button" title="Send" type="submit">Send</button>

        <script>

          $("textarea").val("");

          $("input").val("");

        </script>

